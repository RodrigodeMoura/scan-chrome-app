{
  "name": "Scan",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "default_locale": "en",
  "icons": {
    "32": "assets/icon_32.png",
    "128": "assets/icon_128.png",
    "256": "assets/icon_256.png"
  },
  "permissions": ["documentScan", "fileSystemProvider", "storage"]
}
